   "outputs": [],
   "source": [
    "class scraphtml:\n",
    "    \n",
    "    def __init__(self,soupPage):\n",
    "        self.soupPage = soupPage\n",
    "        \n",
    "    @property\n",
    "    def dictentry(self):\n",
    "        htmldict['item1'] = self.soupPage.title.text\n",
    "        htmldict['item2'] = self.soupPage.link['href']\n",
    "        htmldict['item3'] = self.soupPage.noframes.text\n",
    "        return htmldict\n",
    "\n",
    "nmr = scraphtml(soupPage)"
   ]
  },
