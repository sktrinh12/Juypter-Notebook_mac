    "class scraphtml: #chemspider\n",
    "    \n",
    "    def __init__(self,soupPage):\n",
    "        self.soupPage = soupPage\n",
    "        \n",
    "    @property\n",
    "    def dictentry(self):\n",
    "        htmldict['item1'] = self.soupPage.find('td',{'class':'prop_title'},text='#H bond acceptors:').next_sibling.next_element.text.strip()\n",
    "        htmldict['item2'] = self.soupPage.find('td',{'class':'prop_title'},text='#H bond donors:').next_sibling.next_element.text.strip()\n",
    "        htmldict['item3'] = self.soupPage.find('a',{'href':'http://www.acdlabs.com/logp'}).next_sibling.next_element.next_sibling.text.strip()\n",
    "        return htmldict"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 42,
