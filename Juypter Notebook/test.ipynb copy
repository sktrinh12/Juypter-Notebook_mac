    "class deckofcards:    \n",
    "    suit_lst = [\"Clubs\",\"Diamonds\",\"Hearts\",\"Spades\"]\n",
    "    values_lst = [\"Two\",\"Three\",\"Four\",\"Five\",\"Six\",\"Seven\",\"Eight\",\"Nine\",\"Ten\",\"Jack\",\"Queen\",\"King\",\"Ace\"]\n",
    "\n",
    "    def __init__(self):\n",
    "        self.deck = [[suit,value] for suit in self.suit_lst for value in self.values_lst]\n",
    "        self.cardpack = []\n",
    "        \n",
    "    def IsSuitEmpty(self,suit):\n",
    "        self.suit = suit\n",
    "        self.result = True\n",
    "        if suit in self.suit_lst:\n",
    "            self.result = True\n",
    "        else:\n",
    "            self.result = False\n",
    "        return self.result\n"
   ]
  },
