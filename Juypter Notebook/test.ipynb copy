    "class scraphtml: #pubmedchem\n",
    "    \n",
    "    def __init__(self,soupPage):\n",
    "        self.soupPage = soupPage\n",
    "        \n",
    "    @property\n",
    "    def dictentry(self):\n",
    "        htmldict['item1'] = self.soupPage.div.div.a\n",
    "        htmldict['item2'] = self.soupPage.div.span\n",
    "        htmldict['item3'] = self.soupPage.find('a',{'class':'secondary'})\n",
    "        return htmldict"
   ]
